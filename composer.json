{
    "name": "gremo/daisyui-twig-components",
    "type": "symfony-bundle",
    "description": "A collection of daisyUI components as Symfony UX Twig Components.",
    "keywords": [
        "symfony-ux",
        "twig",
        "components",
        "daisyui"
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/twig-bundle": "^6.4|^7.0",
        "symfony/ux-twig-component": "^2.18"
    },
    "require-dev": {
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpstan/phpstan": "^1.11",
        "squizlabs/php_codesniffer": "^3.10",
        "symfony/css-selector": "^6.4|^7.0",
        "symfony/dom-crawler": "^6.4|^7.0",
        "symfony/finder": "^6.4|^7.0",
        "symfony/phpunit-bridge": "^6.4|^7.0",
        "twig/twig": "^3.10"
    },
    "autoload": {
        "psr-4": {
            "Gremo\\DaisyUITwigComponents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gremo\\DaisyUITwigComponents\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
